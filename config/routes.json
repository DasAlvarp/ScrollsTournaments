{
	"routes": {
		"index": {
			"path": "/",
			"action": "Index::index"
		},
		"tournament_new": {
			"path": "/tournament/new",
			"action": "Tournament::new"
		},
		"tournament_save": {
			"path": "/tournament/save",
			"action": "Tournament::save",
			"method": "POST"
		},
		"tournament_view": {
			"path": "/tournament/{name}",
			"action": "Tournament::view",
			"requirements": {
				"name": "[a-z0-9-]+"
			}
		},
		"tournament_enter": {
			"path": "/tournament/{name}/enter",
			"action": "Tournament::enter",
			"requirements": {
				"name": "[a-z0-9-]+"
			}
		},
		"tournament_invite": {
			"path": "/invite/{code}",
			"action": "Tournament::acceptInvite",
			"requirements": {
				"code": "[a-z0-9]{10}"
			}
		},
		"tournament_result": {
			"path": "/tournament/{name}/results",
			"action": "Tournament::viewResult",
			"requirements": {
				"name": "[a-z0-9-]+"
			}
		},
		"view_match": {
			"path": "/match",
			"action": "Match::viewMatch"
		},
		"login": {
			"path": "/login",
			"action": "User::login"
		},
		"login_post": {
			"path": "/login",
			"action": "User::doLogin",
			"method": "POST"
		},
		"logout": {
			"path": "/logout",
			"action": "User::doLogout"
		},
		"resource_cache": {
			"path": "/assets/cache/{filetype}/{filename}.{filetype}",
			"requirements": {
				"filetype": "css|js",
				"filename": "[a-zA-Z0-9-]+"
			},
			"action": "Resource::assetCache"
		},
		"404": {
			"path": "/404",
			"action": "Index::p404"
		}
	}
}