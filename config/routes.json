{
	"routes": {
		"index": {
			"path": "/",
			"action": "Index::index"
		},
		"tournament_create": {
			"path": "/tournament/create",
			"action": "Tournament::create"
		},
		"tournament_result": {
			"path": "/tournament/{name}/results",
			"action": "Tournament::viewResult",
			"requirements": {
				"name": "[a-z0-9-]+"
			}
		},
		"view_match": {
			"path": "/match",
			"action": "Match::viewMatch"
		},
		"resource_cache": {
			"path": "/assets/cache/{filetype}/{filename}.{filetype}",
			"requirements": {
				"filetype": "css|js",
				"filename": "[a-zA-Z0-9-]+"
			},
			"action": "Resource::assetCache"
		},
		"404": {
			"path": "/404",
			"action": "Index::p404"
		}
	}
}