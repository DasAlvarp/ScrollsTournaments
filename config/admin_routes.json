{
	"routes": {
		"admin_index": {
			"path": "/admin",
			"action": "Admin::index"
		},
		"admin_calendar": {
			"path": "/admin/calendar",
			"action": "Admin::calendar"
		},
		"admin_new_event": {
			"path": "/event/new",
			"action": "Admin::newEvent"
		},
		"admin_edit_event": {
			"path": "/event/{id}",
			"action": "Admin::editEvent",
			"requirements": {
				"id": "[0-9]+"
			}
		},
		"admin_login": {
			"path": "/admin/login",
			"action": "Admin::login"
		},
		"admin_login_post": {
			"path": "/admin/login",
			"action": "Admin::doLogin",
			"method": "POST"
		},
		"admin_logout": {
			"path": "/admin/logout",
			"action": "Admin::doLogout"
		}
	}
}